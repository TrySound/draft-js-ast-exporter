{
  "name": "draft-js-exporter",
  "version": "0.0.1",
  "description": "A generic content exporter for draft-js.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "compile": "npm run build",
    "precompile": "npm run clean",
    "prepublish": "npm run compile",
    "test": "NODE_ENV=test babel-node test | faucet",
    "posttest": "npm run lint",
    "clean": "rm -rf ./lib/*",
    "lint": "eslint 'src/*.js' 'src/**/*.js'; exit 0",
    "watch": "wr 'npm run build' ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icelab/draft-js-exporter.git"
  },
  "keywords": [
    "draft-js",
    "export"
  ],
  "authors": [
    "Max Wheeler <max@icelab.com.au> (https://github.com/makenosound)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icelab/draft-js-exporter/issues"
  },
  "homepage": "https://github.com/icelab/draft-js-exporter",
  "dependencies": {
    "draft-js-utils": "0.1.4"
  },
  "peerDependencies": {
    "draft-js": ">=0.5.0"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-preset-es2015": "6.6.0",
    "eslint": "2.8.0",
    "eslint-config-standard": "5.1.0",
    "eslint-plugin-standard": "1.3.2",
    "faucet": "0.0.1",
    "is-function": "1.0.1",
    "tape": "4.5.1",
    "wr": "^1.3.1"
  }
}
